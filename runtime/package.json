    "@loopback/context": "^5.0.3",
    "axios": "^0.27.2",
    "fastify": "^4.6.0",
    "fastify-http-errors-enhanced": "^4.0.3",
    "reflect-metadata": "^0.1.13",
    "signale": "^1.4.0",
    "sqlite3": "^5.1.1",
    "typeorm": "^0.3.10",
    "undici": "^5.10.0",
    "workerd": "^1.20220926.3"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
