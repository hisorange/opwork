  "license": "GPL-3.0",
  "type": "module",
  "private": false,
  "scripts": {
    "build:server": "tsc -p tsconfig.json",
    "build:admin": "vite build",
    "dev": "nodemon -I --exec node --experimental-specifier-resolution=node --loader ts-node/esm --no-warnings src/server/main.ts",
